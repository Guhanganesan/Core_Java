package Test;


public class Test{
	
   public static void main(String[] args) {

			String tc="Tamil";

			String temp="";
			for( int i=tc.length()-1; i>=0;i--) {

				temp=temp+tc.charAt(i);
			}

			if(temp.equals(tc)) {

				System.out.println("Anagram");
			}
			else {

				System.out.println("Not Anagaram");
			}
        
	}
}
