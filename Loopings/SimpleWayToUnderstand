55555	i=rows		i=5		 j=i  print(i)	j=5,5,5,5,5	 
4444			4   				j=4,4,4,4 	  	
333			3				j=3,3,3		 
22			2				j=2,2 		 
1			1				j=1			 
		         (i>=1)				(j>=1)    
			
public class Main
{
	public static void main(String[] args) {
		int rows=5;
		for (int i=rows; i>=1; i--)
		{
		    for(int j=i; j>=1; j--)
		    {
		        System.out.print(i);
		    }
		    System.out.println();
		}
	}
}
