public class Main
{
	public static void main(String[] args) {
		int rows=5;
		for (int i=1; i<=rows; i++)
		{
		    for(int j=i; j<=rows; j++)
		    {
		        System.out.print(j);
		    }
		    System.out.println();
		}
	}
}

