54321				5	r=5,4,3,2,1

public class Main
{
	public static void main(String[] args) {
		int rows=5;
		for (int i=1; i<=rows; i++)
		{
		    for(int j=rows; j>=i; j--)
		    {
		        System.out.print(j);
		    }
		    System.out.println();
		}
	}
}

